{
    "name": "stepanenko3/laravel-planpay",
    "description": "Laravel Plan Pay package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Artem Stepanenko",
            "email": "stpnnk.artem@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "dompdf/dompdf": "^0.8.0",
        "illuminate/contracts": "~5.8.0|^6.0|^7.0",
        "illuminate/database": "~5.8.0|^6.0|^7.0",
        "illuminate/http": "~5.8.0|^6.0|^7.0",
        "illuminate/log": "~5.8.0|^6.0|^7.0",
        "illuminate/notifications": "~5.8.0|^6.0|^7.0",
        "illuminate/routing": "~5.8.0|^6.0|^7.0",
        "illuminate/support": "~5.8.0|^6.0|^7.0",
        "illuminate/view": "~5.8.0|^6.0|^7.0",
        "moneyphp/money": "^3.2",
        "nesbot/carbon": "^2.0",
        "symfony/http-kernel": "^4.3",
        "symfony/intl": "^4.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.8|^4.0|^5.0",
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Stepanenko3\\PlanPay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stepanenko3\\PlanPay\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "laravel": {
            "providers": [
                "Stepanenko3\\PlanPay\\PlanPayServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
